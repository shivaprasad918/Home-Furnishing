        <main class="main">
            <div class="container">
                <form id="addressForm" action="/add-address" method="post" class="compact-form">
                    <div class="form-section">
                        <h2 class="checkout-title">Billing Details</h2>
                        <div class="form-group">
                            <label for="first-name">Name *</label>
                            <input type="text" class="form-control" id="first-name" name="name" required
                                   pattern="[a-zA-Z\s]+" title="Name should only contain letters and spaces."
                                   autocomplete="off">
                            <div class="invalid-feedback">Please enter a valid name (letters and spaces only).</div>
                        </div>
                        <div class="form-group">
                            <label for="phone">Phone Number *</label>
                            <input type="tel" class="form-control" id="phone" name="phone" required pattern="[0-9]{10}"
                                   title="Phone number should be 10 digits."
                                   autocomplete="off">
                            <div class="invalid-feedback">Please enter a valid 10-digit phone number.</div>
                        </div>
                        <div class="form-group">
                            <label for="building-name">Building Name/House Name *</label>
                            <input type="text" class="form-control" id="building-name" name="buildingName" required
                                   autocomplete="off">
                            <div class="invalid-feedback">Please enter the building or house name.</div>
                        </div>
                        <div class="form-group">
                            <label for="city">City *</label>
                            <input type="text" class="form-control" id="city" name="city" required
                                   autocomplete="off">
                            <div class="invalid-feedback">Please enter the city.</div>
                        </div>
                        <div class="form-group">
                            <label for="district">District *</label>
                            <input type="text" class="form-control" id="district" name="district" required
                                   autocomplete="off">
                            <div class="invalid-feedback">Please enter the district.</div>
                        </div>
                        <div class="form-group">
                            <label for="state">State *</label>
                            <input type="text" class="form-control" id="state" name="state" required
                                   autocomplete="off">
                            <div class="invalid-feedback">Please enter the state.</div>
                        </div>
                        <div class="form-group">
                            <label for="postcode">Pincode *</label>
                            <input type="text" class="form-control" id="postcode" name="postcode" required
                                   pattern="[0-9]{6}" title="Pincode should be 6 digits."
                                   autocomplete="off">
                            <div class="invalid-feedback">Please enter a valid 6-digit pincode.</div>
                        </div>
                    </div>
                    <div class="button-group">
                        <button type="submit" class="btn btn-primary">Save Address</button>
                    </div>
                </form>
            </div>
        </main>
        
        <script>
            document.getElementById('addressForm').addEventListener('submit', function (event) {
                event.preventDefault(); // Prevent the form from submitting initially
        
                if (validateForm()) {
                    this.submit(); // If validation passes, submit the form
                }
            });
        
            function validateForm() {
                let isValid = true;
        
                // Reset all input fields' validity
                document.querySelectorAll('.form-control').forEach(function (input) {
                    input.classList.remove('is-invalid');
                });
        
                // Validate each field
                let nameInput = document.getElementById('first-name');
                let phoneInput = document.getElementById('phone');
                let buildingNameInput = document.getElementById('building-name');
                let cityInput = document.getElementById('city');
                let districtInput = document.getElementById('district');
                let stateInput = document.getElementById('state');
                let postcodeInput = document.getElementById('postcode');
        
                // Trim inputs to remove leading/trailing spaces
                nameInput.value = nameInput.value.trim();
                buildingNameInput.value = buildingNameInput.value.trim();
                cityInput.value = cityInput.value.trim();
                districtInput.value = districtInput.value.trim();
                stateInput.value = stateInput.value.trim();
                postcodeInput.value = postcodeInput.value.trim();
        
                // Validate each field
                if (!nameInput.checkValidity() || nameInput.value.length === 0) {
                    isValid = false;
                    nameInput.classList.add('is-invalid');
                }
        
                if (!phoneInput.checkValidity() || phoneInput.value.length === 0) {
                    isValid = false;
                    phoneInput.classList.add('is-invalid');
                }
        
                if (!buildingNameInput.checkValidity() || buildingNameInput.value.length === 0) {
                    isValid = false;
                    buildingNameInput.classList.add('is-invalid');
                }
        
                if (!cityInput.checkValidity() || cityInput.value.length === 0) {
                    isValid = false;
                    cityInput.classList.add('is-invalid');
                }
        
                if (!districtInput.checkValidity() || districtInput.value.length === 0) {
                    isValid = false;
                    districtInput.classList.add('is-invalid');
                }
        
                if (!stateInput.checkValidity() || stateInput.value.length === 0) {
                    isValid = false;
                    stateInput.classList.add('is-invalid');
                }
        
                if (!postcodeInput.checkValidity() || postcodeInput.value.length === 0) {
                    isValid = false;
                    postcodeInput.classList.add('is-invalid');
                }
        
                return isValid;
            }
        </script>
       

