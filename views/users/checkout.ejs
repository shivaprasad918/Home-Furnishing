                                                                            <td>Subtotal:</td>
                                                                            <td>
                                                                                <%= subtotal.toFixed(2) %>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>Shipping:</td>
                                                                            <td>Free shipping</td>
                                                                        </tr>
