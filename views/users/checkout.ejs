                                            <!------------------------------------------coupon add and remove script ------------------------------------>


                                            <script>
                                                document.getElementById('checkout-discount-input').addEventListener('click', function () {
                                                    $('#couponModal').modal('show');
                                                });

                                                document.getElementById('applyCouponButton').addEventListener('click', function () {
                                                    const selectedCoupon = document.getElementById('couponSelect').selectedOptions[0];
                                                    const couponCode = selectedCoupon.value;

                                                    fetch('/applyCoupon', {
                                                        method: 'POST',
                                                        headers: {
                                                            'Content-Type': 'application/json'
                                                        },
                                                        body: JSON.stringify({ couponCode })
                                                    })
                                                        .then(response => response.json())
                                                        .then(data => {
                                                            if (data.message === 'Coupon applied successfully') {
                                                                const cart = data.cart;
                                                                let grandTotal = cart.grandTotal;

                                                                document.getElementById('grandTotal').innerText = grandTotal.toFixed(2);
                                                                document.getElementById('checkout-discount-input').value = couponCode;
                                                                document.getElementById('hidden-coupon-code').value = couponCode;

                                                                // Close the modal
                                                                $('#couponModal').modal('hide');
                                                            } else {
                                                                alert(data.message);
                                                            }
                                                        })
                                                        .catch(error => {
                                                            console.error('Error:', error);
                                                            alert('An error occurred while applying the coupon.');
                                                        });
                                                });

                                                // Ensure coupon code is included in form submission
                                                document.getElementById('order-form').addEventListener('submit', function (event) {
                                                    const couponCode = document.getElementById('checkout-discount-input').value;
                                                    document.getElementById('hidden-coupon-code').value = couponCode;
                                                });

                                                // Function to remove the coupon and update the grand total
                                                function removeCoupon() {
                                                    // Reset the grand total to its original value
                                                    document.getElementById('grandTotal').innerText = '<%= grandTotal %>';

                                                    // Clear the coupon code fields
                                                    document.getElementById('checkout-discount-input').value = '';
                                                    document.getElementById('hidden-coupon-code').value = '';

                                                    // Ensure the form submission will not include a coupon code
                                                    document.getElementById('hidden-coupon-code').value = '';
                                                    window.location.reload()
                                                }
                                            </script>

