                        </div><!-- End .checkout-discount -->



                        <!-- Modal for selecting coupon -->
                        <div class="modal fade" id="couponModal" tabindex="-1" role="dialog"
                            aria-labelledby="couponModalLabel" aria-hidden="true">
                            <div class="modal-dialog" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="couponModalLabel">Select a Coupon</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <select id="couponSelect" class="form-control">
                                            <option value="" selected disabled>Select a Coupon</option>
                                            <% coupons.forEach(coupon=> { %>
                                                <option value="<%= coupon.couponCode %>"
                                                    data-discount="<%= coupon.discount %>">
                                                    <%= coupon.couponName %>
                                                </option>
                                                <% }); %>
                                        </select>

                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-secondary"
                                            data-dismiss="modal">Cancel</button>
                                        <button type="button" class="btn btn-primary"
                                            id="applyCouponButton">Apply</button>
                                    </div>
                                </div>
                            </div>
                        </div>

