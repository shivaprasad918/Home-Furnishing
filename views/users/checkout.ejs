                                                        <p>
                                                            <input type="radio" id="billing-address<%= index + 1 %>"
                                                                name="billing-address" value="<%= index %>" <% if
                                                                (index===address.length - 1) { %>checked<% } %>>
                                                                <label for="billing-address<%= index + 1 %>">Select this
                                                                    address</label>
                                                                <br>
                                                                <strong>Name:</strong>
                                                                <%= item.name %><br>
                                                                    <strong>Phone:</strong>
                                                                    <%= item.phone %><br>
                                                                        <strong>Address:</strong>
                                                                        <%= item.buildingName %>, <%= item.city %><br>
                                                                                <%= item.district %>, <%= item.state %>
                                                                                        -
                                                                                        <%= item.postcode %><br>
                                                                                            <!-- <a href="/editAddress/<%= item._id %>">Edit <i class="icon-edit"></i></a> -->
                                                                                            <button class="btn btn-link"
                                                                                                onclick="deleteAddress('<%= item._id %>')">Delete
                                                                                                <i
                                                                                                    class="icon-delete"></i></button>
                                                        </p>
                                                    </div><!-- End .card-body -->
                                                </div><!-- End .card-dashboard -->
